// PerlinNoise3D:2
	float n_out2p0;
	{
		n_out2p0 = cnoise(vec3((n_out4p0.xy + n_out5p0.xy) * n_out12p0, n_out7p0)) * 0.5 + 0.5
	}
// VectorOp:3
	vec3 n_out3p0;
	n_out3p0 = n_out8p0 - vec3(n_out2p0);

// Output:0
	VERTEX = n_out3p0;

}

